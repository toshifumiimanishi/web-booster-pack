    "gulp-data": "^1.3.1",
    "gulp-ejs": "^5.1.0",
    "gulp-eslint": "^6.0.0",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-sass-variables": "^1.2.0",
    "gulp-typescript": "^5.0.1",
    "jest": "^26.4.2",
    "jest-image-snapshot": "^4.2.0",
    "jest-puppeteer": "^4.1.1",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-reporter": "^6.0.0",
    "postcss-scss": "^2.1.1",
