  "jest": {
    "preset": "jest-puppeteer",
    "setupFilesAfterEnv": [
      "./test/e2e/jest-setup-test-framework.js"
    ]
  },
  "browserslist": [
    "last 2 versions",
    "ie >= 11",
    "android >= 4.4.4",
    "not dead"
